<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_config_settings_view_form_debranding_custom" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.debranding.custom</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="6"/>
        <field name="inherit_id" ref="brainpack_debranding.res_config_settings_view_form_debranding"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@data-key='brainpack_debranding']/div[1]" position="inside">
                <div class="col-12 col-lg-6 o_setting_box">
                    <label for="pos_logo" class="col-3 o_light_label"/>
                    <field name="pos_logo" widget="image" class="oe_avatar" />
                </div>
            </xpath>
        </field>
    </record>

</odoo>